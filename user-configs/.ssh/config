Host personal.bitbucket.org
  HostName bitbucket.org
  User git
  IdentityFile ~/.ssh/id_rsa_personal
  IdentitiesOnly yes
  AddKeysToAgent yes

Host personal.github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa_personal
  IdentitiesOnly yes
  AddKeysToAgent yes

Host personal.gitlab.com
  HostName gitlab.com
  User git
  IdentityFile ~/.ssh/id_rsa_personal
  IdentitiesOnly yes
  AddKeysToAgent yes

